    "version": "0.2.0",
    "configurations": [
        {
            "name": "Main Run",
            "type": "python",
            "request": "launch",
            "env": { "PYTHONPATH": "${workspaceRoot}" },
            "cwd": "${workspaceRoot}",
            "program": "${workspaceFolder}\\main.py",
            "console": "integratedTerminal"
          },
          {
            "name": "Simulate Server",
            "type": "python",
            "request": "launch",
            "env": { "PYTHONPATH": "${workspaceRoot}" },
            "cwd": "${workspaceRoot}",
            "program": "${workspaceFolder}\\main.py",
            "console": "integratedTerminal",
            "args": [
              "-name",
              "testing_name-0"
            ]
          },
          {
            "name": "Generate Experiments",
            "type": "python",
            "request": "launch",
            "env": { "PYTHONPATH": "${workspaceRoot}" },
            "cwd": "${workspaceRoot}",
            "program": "${workspaceFolder}\\Utils\\generateExperiments.py",
            "console": "integratedTerminal"
          },
          {
            "name": "Create Causal Graph",
            "type": "python",
            "request": "launch",
            "env": { "PYTHONPATH": "${workspaceRoot}" },
            "cwd": "${workspaceRoot}",
            "program": "${workspaceFolder}\\createCausalGraph.py",
            "console": "integratedTerminal"
          },
          {
            "name": "Create All Graphs",
            "type": "python",
            "request": "launch",
            "env": { "PYTHONPATH": "${workspaceRoot}" },
            "cwd": "${workspaceRoot}",
            "program": "${workspaceFolder}\\allGraphs.py",
            "console": "integratedTerminal"
          }
    ]
}